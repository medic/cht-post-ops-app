{
  "name": "zazic-scale-up",
  "version": "0.1.0",
  "description": "",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "eslint": "eslint **.js",
    "test": "npm run eslint && npm run test-unit",
    "test-contact-summary": "medic-contact-summary-test",
    "test-unit": "mocha --reporter progress test/*/*.spec.js --timeout 10000",
    "travis": "npm install && npm test",
    "deploy-app-forms": "medic-conf --url=$url convert-app-forms upload-app-forms -- scheduled_msgs client_review no_contact referral_for_care",
    "deploy-contact-forms": "medic-conf --url=$url convert-contact-forms upload-contact-forms -- person-create --person-edit",
    "deploy-app-settings": "medic-conf --url=$url compile-app-settings upload-app-settings",
    "deploy-translations": "medic-conf --url=$url upload-custom-translations",
    "deploy-resources": "medic-conf --url=$url upload-resources",
    "deploy": "npm-run-all deploy-translations deploy-app-forms deploy-contact-forms deploy-app-settings deploy-resources"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-exclude": "^1.0.8",
    "chai-like": "^1.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^6.8.0",
    "eslint-plugin-json": "^2.1.1",
    "medic-conf": "^3.2.0",
    "medic-conf-test-harness": "^0.1.30",
    "mocha": "^5.1.1",
    "npm-run-all": "^4.1.5"
  }
}
